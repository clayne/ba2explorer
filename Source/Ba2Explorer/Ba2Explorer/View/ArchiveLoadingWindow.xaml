<Window x:Class="Ba2Explorer.View.ArchiveLoadingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ba2Explorer.View"
        mc:Ignorable="d"
        Title="Loading..." Height="100" Width="300" WindowStartupLocation="CenterOwner" ResizeMode="NoResize">
    <StackPanel Orientation="Vertical" Margin="10">
        <Label Padding="0,0,0,10">Loading...</Label>
        <ProgressBar IsIndeterminate="True" Height="16"/>
    </StackPanel>
</Window>
